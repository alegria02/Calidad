<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:web="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" id="WebApp_ID" version="3.0">
  <display-name>Calidad</display-name>
  <welcome-file-list>
    <welcome-file>index.jsp</welcome-file>
  </welcome-file-list>
  <servlet>
    <servlet-name>DatosServlet</servlet-name>
    <servlet-class>com.proceso.servlet.DatosServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>DatosServlet</servlet-name>
    <url-pattern>/Datos</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>CargaLog4j</servlet-name>
    <servlet-class>com.proceso.servlet.CargaLog4j</servlet-class>
    <init-param>
      <param-name>log4jPropertiesFile</param-name>
      <param-value>/home/planillas/conf/log4j.properties</param-value>
    </init-param>
    <load-on-startup>1</load-on-startup>
  </servlet>
</web-app>